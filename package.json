{
  "name": "my-app",
  "version": "1.0.0",
  "main": "./main.electron.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node ./cli.js dev",
    "build": "react-router build && npx electron-forge make",
    "icons:generate": "icon-gen -i ./icons/icon.svg -o ./icons -r --ico --icns",
    "types:check": "react-router typegen && tsc --noEmit",
    "types:show-config": "tsc --showConfig",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "code:check": "npm run types:check && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@react-router/node": "^7.1.1",
    "isbot": "^5.1.19",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@eslint/config-inspector": "^0.6.0",
    "@eslint/js": "^9.17.0",
    "@react-router/dev": "^7.1.1",
    "@react-router/fs-routes": "^7.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "commander": "^12.1.0",
    "electron": "^33.2.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "icon-gen": "^5.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.6",
    "vite-imagetools": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
